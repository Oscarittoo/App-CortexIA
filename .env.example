# CORTEXIA - Configuration des API de Visioconférence
# Copiez ce fichier vers .env et remplissez vos clés API

# =============================================================================
# ZOOM API
# =============================================================================
# Documentation: https://marketplace.zoom.us/docs/api-reference/introduction
VITE_ZOOM_CLIENT_ID=your_zoom_client_id_here
VITE_ZOOM_CLIENT_SECRET=your_zoom_client_secret_here
VITE_ZOOM_ACCOUNT_ID=your_zoom_account_id_here

# =============================================================================
# GOOGLE MEET API (Google Workspace)
# =============================================================================
# Documentation: https://developers.google.com/meet/api
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here.apps.googleusercontent.com
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret_here
VITE_GOOGLE_API_KEY=your_google_api_key_here

# =============================================================================
# MICROSOFT TEAMS API
# =============================================================================
# Documentation: https://learn.microsoft.com/en-us/graph/api/resources/teams-api-overview
VITE_TEAMS_CLIENT_ID=your_teams_client_id_here
VITE_TEAMS_CLIENT_SECRET=your_teams_client_secret_here
VITE_TEAMS_TENANT_ID=your_teams_tenant_id_here

# =============================================================================
# WEBEX API
# =============================================================================
# Documentation: https://developer.webex.com/docs/api/getting-started
VITE_WEBEX_CLIENT_ID=your_webex_client_id_here
VITE_WEBEX_CLIENT_SECRET=your_webex_client_secret_here

# =============================================================================
# SLACK HUDDLES API
# =============================================================================
# Documentation: https://api.slack.com/
VITE_SLACK_CLIENT_ID=your_slack_client_id_here
VITE_SLACK_CLIENT_SECRET=your_slack_client_secret_here
VITE_SLACK_SIGNING_SECRET=your_slack_signing_secret_here

# =============================================================================
# DISCORD API
# =============================================================================
# Documentation: https://discord.com/developers/docs/intro
VITE_DISCORD_CLIENT_ID=your_discord_client_id_here
VITE_DISCORD_CLIENT_SECRET=your_discord_client_secret_here
VITE_DISCORD_BOT_TOKEN=your_discord_bot_token_here

# =============================================================================
# TRANSCRIPTION APIs
# =============================================================================

# OpenAI Whisper API
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Deepgram API
VITE_DEEPGRAM_API_KEY=your_deepgram_api_key_here

# AssemblyAI API
VITE_ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here

# Google Cloud Speech-to-Text
VITE_GOOGLE_SPEECH_API_KEY=your_google_speech_api_key_here

# Azure Speech Services
VITE_AZURE_SPEECH_KEY=your_azure_speech_key_here
VITE_AZURE_SPEECH_REGION=your_azure_region_here

# =============================================================================
# AI SUMMARIZATION APIs
# =============================================================================

# OpenAI GPT (pour résumés)
VITE_OPENAI_MODEL=gpt-4-turbo-preview

# Anthropic Claude
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Cohere
VITE_COHERE_API_KEY=your_cohere_api_key_here

# =============================================================================
# STORAGE & DATABASE
# =============================================================================

# Supabase (optionnel - pour sync cloud)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Firebase (optionnel)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id_here

# =============================================================================
# PRODUCTIVITY INTEGRATIONS
# =============================================================================

# Notion API (export de comptes-rendus)
VITE_NOTION_API_KEY=your_notion_api_key_here

# Trello API (création de cartes depuis actions)
VITE_TRELLO_API_KEY=your_trello_api_key_here
VITE_TRELLO_TOKEN=your_trello_token_here

# Asana API
VITE_ASANA_ACCESS_TOKEN=your_asana_token_here

# Jira API
VITE_JIRA_API_TOKEN=your_jira_api_token_here
VITE_JIRA_DOMAIN=your-domain.atlassian.net

# Linear API
VITE_LINEAR_API_KEY=your_linear_api_key_here

# =============================================================================
# EMAIL SERVICES
# =============================================================================

# SendGrid (envoi de comptes-rendus)
VITE_SENDGRID_API_KEY=your_sendgrid_api_key_here

# Mailgun
VITE_MAILGUN_API_KEY=your_mailgun_api_key_here
VITE_MAILGUN_DOMAIN=your_mailgun_domain_here

# =============================================================================
# CALENDAR INTEGRATIONS
# =============================================================================

# Google Calendar
VITE_GOOGLE_CALENDAR_API_KEY=your_google_calendar_key_here

# Microsoft Outlook Calendar
VITE_OUTLOOK_CLIENT_ID=your_outlook_client_id_here

# =============================================================================
# ANALYTICS (optionnel)
# =============================================================================

# Mixpanel
VITE_MIXPANEL_TOKEN=your_mixpanel_token_here

# Amplitude
VITE_AMPLITUDE_API_KEY=your_amplitude_key_here

# Posthog
VITE_POSTHOG_KEY=your_posthog_key_here
VITE_POSTHOG_HOST=https://app.posthog.com
